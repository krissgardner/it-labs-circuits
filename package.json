{
  "name": "it-labs_ui",
  "version": "1.0.0",
  "description": "some circuit ideas for the front page",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/style.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    
    "compile:sass": "node-sass sass/style.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krissrobert12/it-labs-circuits.git"
  },
  "author": "Kriss Gardner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krissrobert12/it-labs-circuits/issues"
  },
  "homepage": "https://github.com/krissrobert12/it-labs-circuits#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "live-server": "^1.2.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^7.1.1"
  }
}
